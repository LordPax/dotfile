" Options
set mouse=
set mouse+=pageininsert
set mouse+=pageoutsideinsert
set mouse+=switchtab
set mouse+=history
set mouse+=guiontop
set mouse+=newtab
set mouse+=closetab
set mouse+=menupage
set mouse+=menusuggest
set mouse+=menuvieb
set mouse+=modeselector
set mouse+=movepointer
set mouse+=follow
set mouse+=toinsert
set mouse+=toexplore
set mouse+=leaveinput
set mouse+=suggestselect
set mouse+=scrollsuggest
set mouse+=scrollzoom
set mouse+=scrolltabs
set mouse+=screenshotframe
set permissioncertificateerror=allow

" Commands
colorscheme matcha-dark

" Mappings
unmap m<Any>
unmap M<Any>
unmap q<Any>

nmap j <Left>
nmap k <Down>
nmap l <Up>
nmap m <Right>
nmap q :close<cr>
nmap d <scrollPageDownHalf>
nmap u <scrollPageUpHalf>
nmap U <restoretabs>
" nmap f <scrollPageDownHalf>
" nmap b <scrollPageUpHalf>

nmap J <backInHistory>
nmap M <forwardInHistory>

nmap <C-i> <previousTab>
nmap <C-o> <nextTab>
nmap I <moveTabBackward>
nmap O <moveTabForward>

nmap <C-w>j <toLeftSplitWindow>
nmap <C-w>k <toBottomSplitWindow>
nmap <C-w>l <toTopSplitWindow>
nmap <C-w>m <toRightSplitWindow>

